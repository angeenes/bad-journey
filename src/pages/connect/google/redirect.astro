<script>

    import { WEB_APP_URL } from "../../../consts.js";

    function getUrlParameters(): { [key: string]: string } {
        var params = {};
        window.location.search.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(str, key, value) {
            params[key] = value;
        });
        return params;
    }

    function retrieveUserInfos() {
        const params = getUrlParameters();
        console.log(params);
        this.http.get(`${WEB_APP_URL}/auth/google/callback`, { withCredentials: true }).subscribe((res) => {
            console.log(res);
        });
    }

    retrieveUserInfos();
</script>