<body>
    REDIRECT FROM GOOGLE
</body>
</html>

<script>

    import { GoogleOauth } from "../../../assets/types/googleOauth.js";
    import { API_URL } from "../../../consts.js";

    function getUrlParameters(): GoogleOauth {
        var params = {};
        window.location.search.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(str, key, value) {
            params[key] = value;
        });
        return params as GoogleOauth;
    }

    function retrieveUserInfos() {
        const params: GoogleOauth = getUrlParameters();
        console.log(params.access_token);
        this.http.get(`${API_URL}auth/google/callback?access_token=${params.access_token}`).subscribe((res) => {
            console.log(res);
        });
    }

    retrieveUserInfos();
</script>